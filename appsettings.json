{
  "Monitor": {
    "port": "COM6",
    "writeRawData" : true
    //"replayCycle" : 5,
    //"outputDir" : ""
  },
  "NLog": {
    "targets": {
      "console": {
        "type": "ColoredConsole",
        "layout": "${message:withException=true}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "console"
      }
    ]
  }
}
